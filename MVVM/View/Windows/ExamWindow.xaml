<Window x:Class="ZabgcExamsDesktop.MVVM.View.Windows.ExamWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZabgcExamsDesktop.MVVM.View.Windows"
        mc:Ignorable="d"
        Title="Добавление экзамена" Height="400" Width="350" WindowStartupLocation="CenterScreen" WindowStyle="None"
        Background="#323a47"
        ResizeMode="CanMinimize"
        >
    <Grid MouseDown="ToolBar_MouseDown">
        <Image x:Name="closeBtn" Grid.Row="0" Source="/Icons/Знак_закрыть.png"  Panel.ZIndex="10" MouseDown="closeBtn_MouseDown" RenderOptions.BitmapScalingMode="HighQuality" Margin="321,1,1,371"/>
        <Image x:Name="colapseBtn"  Grid.Row="0" Source="/Icons/Знак_свернуть.png" Panel.ZIndex="10" MouseDown="colapseBtn_MouseDown" RenderOptions.BitmapScalingMode="HighQuality" Margin="289,-1,33,369"/>
        <Frame Source="/MVVM/View/Pages/ExamPage.xaml" x:Name="mainFrame" Panel.ZIndex="1" Margin="-2,-5,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
